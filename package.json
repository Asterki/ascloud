{
	"name": "ascloud",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon --config ./nodemon.json ./server/index.ts",
		"start": "npm-run-all start:*",
		"start:clean": "rm -rf ./build/*",
		"start:build-server": "tsc -p ./server/tsconfig.json",
		"start:build-client": "next build",
		"start:run-server": "cross-env NODE_ENV=production node ./build/server/index.js",
		"build": "next build",
		"lint": "next lint && npx eslint"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.5",
