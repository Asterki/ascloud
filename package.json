		"bcrypt": "^5.1.0",
		"compression": "^1.7.4",
		"connect-mongo": "^5.0.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.1.4",
		"eslint": "8.42.0",
		"eslint-config-next": "13.4.4",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"locale": "^0.1.0",
		"mongoose": "^7.2.2",
		"next": "13.4.4",
		"nodemailer": "^6.9.3",
		"passport": "^0.6.0",
